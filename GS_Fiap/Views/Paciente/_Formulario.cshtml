@model Paciente


<div class="campos">
    <label asp-for="Nome">Nome : </label>

    <input asp-for="Nome" type="text" size="40" maxlength="130" required autocomplete="off">


    <label asp-for="DataNascimento">Data de nascimento: </label>

    <input type="date" asp-for=" DataNascimento" min=" 1920-01-01" max="2023-11-22" required>
</div>
<div class="campos">
    <label asp-for="Senha">Senha: </label>
    <input asp-for="Senha" type="text" size="40" maxlength="40" required autocomplete="off">
    <label asp-for="CPF">CPF: </label>
    <input asp-for="CPF" type="text" size="33" maxlength="11" required autocomplete="off">
</div>